<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sc.dao.ShopDao">
	<insert id="inserShop" useGeneratedKeys="true"
		keyColumn="shop_id" keyProperty="shopId">
		insert into
		tb_shop(owner_id,area_id,shop_category_id,shop_name,shop_desc,
		shop_addr,phone,shop_img,priority,enable_status,advice)
		values
		(#{owner.userId},#{area.areaId},#{shopCategory.shopCategoryId},#{shopName},
		#{shopDesc},#{shopAddr},#{phone},#{shopImg},#{priority},#{enableStatus},#{advice})
	</insert>
</mapper>